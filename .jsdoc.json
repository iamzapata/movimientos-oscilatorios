{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
  },
  "source": {
      "include": ["lib", "constantes", "README.md", "componentes"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "opts": {
      "destination": ".public/documentacion",
      "encoding": "utf8",
      "private": true,
      "recurse": true,
      "template": "./node_modules/jsdoc-template"
  }
}